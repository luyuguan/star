<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="com.mysql.jdbc.Driver"/>
        <property name="url" value="jdbc:mysql://127.0.0.1:3306/lygdb"/>
        <property name="username" value="root"/>
        <property name="password" value="lyg"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="xml/usermapper.xml"/>
    <mapper resource="xml/usermapper1.xml"/>
    <mapper resource="xml/usermapper2.xml"/>
    <mapper resource="xml/usermapper3.xml"/>
    <mapper resource="xml/usermapper4.xml"/>
    <mapper resource="xml/usermapper5.xml"/>
    <mapper resource="xml/usermapper6.xml"/>
    <mapper resource="xml/usermapper7.xml"/>
    <mapper resource="xml/usermapper8.xml"/>
    <mapper class="com.lyg.mybatisdb.Mapper"/>
  </mappers>
</configuration>